{
    "swagger": "2.0",
    "info": {
        "description": "API for Reog Group in Ponorogo",
        "title": "Reog Apps API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Erik Rio Setiawan",
